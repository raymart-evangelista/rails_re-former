<%= form_with model: @user, url: users_path(@user), method: :patch do |form| %>
  <%= form.label :username %>
  <%= form.text_field :username %>
  <br>
  <%= form.label :email %>
  <%= form.email_field :email %>
  <br>
  <%= form.label :password %>
  <%= form.password_field :password %>
  <br>
  <%= form.submit "Submit data" %>
<% end %>